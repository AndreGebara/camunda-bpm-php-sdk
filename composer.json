{
  "name": "camunda/camunda-bpm-php-sdk",
  "description": "PHP SDK for camundaBPM process services",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "keywords": ["sdk","camunda", "camunda bpm"],
  "homepage": "http://camunda.org",
  "authors": [
    {
      "name": "Stefan Hentschel",
      "email": "stefan.hentschel@camunda.com"
    }
  ],

  "require": {
    "php": ">=5.3.0"
  },

  "require-dev": {
    "phpunit/phpunit": "3.7.21"
  },

  "archive": {
    "exclude": [
      "/procedual/test",
      "/procedual/test/*",
      "/oop/test",
      "/oop/test/*"
    ]
  },

  "autoload": {
    "psr-0":  {
      "org\\camunda\\php\\sdk\\entity\\request\\": "oop/main/entity/request/",
      "org\\camunda\\php\\sdk\\entity\\response\\": "oop/main/entity/response/",
      "org\\camunda\\php\\sdk\\helper\\": "oop/main/helper/",
      "org\\camunda\\php\\sdk\\service\\": "oop/main/service/",
      "org\\camunda\\php\\sdk": "oop/main/"
    }
  }
}
